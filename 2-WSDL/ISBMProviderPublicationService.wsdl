<?xml version="1.0" encoding="utf-8"?>
<wsdl:definitions
	xmlns:tns="http://www.openoandm.org/xml/ISBM/"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
	xmlns:soap12="http://schemas.xmlsoap.org/wsdl/soap12/"
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	targetNamespace="http://www.openoandm.org/xml/ISBM/">
	<wsdl:types>
		<xs:schema elementFormDefault="qualified" targetNamespace="http://www.openoandm.org/xml/ISBM/" >
			<xs:element name="TransactionStatus">
				<xs:complexType>
					<xs:sequence>
						<xs:element minOccurs="1" maxOccurs="1" name="successOrErrorCriteria">
							<xs:simpleType>
								<xs:restriction base="xs:integer">
									<xs:enumeration value="0"/>
									<xs:enumeration value="1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element minOccurs="1" maxOccurs="1" name="statusMessage" type="xs:string"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ClosePublication">
				<xs:complexType>
					<xs:sequence>
						<xs:element minOccurs="1" maxOccurs="1" name="channelSessionID" type="xs:string" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ClosePublicationResponse" type="tns:ClosePublicationResponseType">
				<xs:complexType name="ClosePublicationResponseType">
					<xs:sequence>
						<xs:element minOccurs="1" maxOccurs="1" ref="tns:TransactionStatus" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="OpenPublication">
				<xs:complexType>
					<xs:sequence>
						<xs:element minOccurs="1" maxOccurs="1" name="channelID" type="xs:string" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:complexType name="OpenPublicationResponseType">
				<xs:sequence> 	
					<xs:element minOccurs="0" maxOccurs="1" name="channelSessionID" type="xs:string" />
					<xs:element minOccurs="0" maxOccurs="1" name="lastMessageID" type="xs:string" />
					<xs:element minOccurs="1" maxOccurs="1" ref="tns:TransactionStatus" />
				</xs:sequence>
			</xs:complexType>
			<xs:element name="OpenPublicationResponse" type="tns:OpenPublicationResponseType" /> 	
			<xs:element name="PublicationMessage">
				<xs:complexType>
					<xs:sequence>
						<xs:any minOccurs="1" maxOccurs="1" namespace="##any" processContents="lax" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="PostPublication">
				<xs:complexType>
					<xs:sequence>
						<xs:element minOccurs="1" maxOccurs="1" name="channelSessionID" type="xs:string" />
						<xs:element minOccurs="1" maxOccurs="1" name="topic" type="xs:string" />
						<xs:element minOccurs="1" maxOccurs="1" ref="tns:PublicationMessage" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:complexType name="PostPublicationResponseType">
				<xs:sequence> 	
					<xs:element minOccurs="0" maxOccurs="1" name="messageID" type="xs:string" />
					<xs:element minOccurs="1" maxOccurs="1" ref="tns:TransactionStatus" />
				</xs:sequence>
			</xs:complexType>
			<xs:element name="PostPublicationResponse" type="tns:PostPublicationResponseType" />
		</xs:schema>
	</wsdl:types>
	<wsdl:message name="ClosePublicationSoapIn">
		<wsdl:part name="parameters" element="tns:ClosePublication" />
	</wsdl:message>
	<wsdl:message name="ClosePublicationSoapOut">
		<wsdl:part name="parameters" element="tns:ClosePublicationResponse" />
	</wsdl:message>
	<wsdl:message name="OpenPublicationSoapIn">
		<wsdl:part name="parameters" element="tns:OpenPublication" />
	</wsdl:message>
	<wsdl:message name="OpenPublicationSoapOut">
		<wsdl:part name="parameters" element="tns:OpenPublicationResponse" />
	</wsdl:message>
	<wsdl:message name="PostPublicationSoapIn">
		<wsdl:part name="parameters" element="tns:PostPublication" />
	</wsdl:message>
	<wsdl:message name="PostPublicationSoapOut">
		<wsdl:part name="parameters" element="tns:PostPublicationResponse" />
	</wsdl:message>
	<wsdl:portType name="ISBMProviderPublicationServiceSoap">
		<wsdl:operation name="ClosePublication">
			<wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Closes a publication channel from posting of messages.</wsdl:documentation>
			<wsdl:input message="tns:ClosePublicationSoapIn" />
			<wsdl:output message="tns:ClosePublicationSoapOut" />
		</wsdl:operation>
		<wsdl:operation name="OpenPublication">
			<wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Determines if a publication Channel exists and returns the channel ID. Opens the publication channel for posting of publication messages.</wsdl:documentation>
			<wsdl:input message="tns:OpenPublicationSoapIn" />
			<wsdl:output message="tns:OpenPublicationSoapOut" />
		</wsdl:operation>
		<wsdl:operation name="PostPublication">
			<wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Posts a publication message to a publication channel.</wsdl:documentation>
			<wsdl:input message="tns:PostPublicationSoapIn" />
			<wsdl:output message="tns:PostPublicationSoapOut" />
		</wsdl:operation>
	</wsdl:portType>
	<wsdl:binding name="ISBMProviderPublicationServiceSoap" type="tns:ISBMProviderPublicationServiceSoap">
		<soap:binding transport="http://schemas.xmlsoap.org/soap/http" />
		<wsdl:operation name="ClosePublication">
			<soap:operation soapAction="http://www.openoandm.org/xml/ISBM/ClosePublication" style="document" />
			<wsdl:input>
				<soap:body use="literal" />
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" />
			</wsdl:output>
		</wsdl:operation>
		<wsdl:operation name="OpenPublication">
			<soap:operation soapAction="http://www.openoandm.org/xml/ISBM/OpenPublication" style="document" />
			<wsdl:input>
				<soap:body use="literal" />
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" />
			</wsdl:output>
		</wsdl:operation>
		<wsdl:operation name="PostPublication">
			<soap:operation soapAction="http://www.openoandm.org/xml/ISBM/PostPublication" style="document" />
			<wsdl:input>
				<soap:body use="literal" />
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" />
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>
	<wsdl:binding name="ISBMProviderPublicationServiceSoap12" type="tns:ISBMProviderPublicationServiceSoap">
		<soap12:binding transport="http://schemas.xmlsoap.org/soap/http" />
