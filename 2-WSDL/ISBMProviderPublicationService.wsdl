			</wsdl:output>
		</wsdl:operation>
		<wsdl:operation name="OpenPublication">
			<soap12:operation soapAction="http://www.openoandm.org/xml/ISBM/OpenPublication" style="document" />
			<wsdl:input>
				<soap12:body use="literal" />
			</wsdl:input>
			<wsdl:output>
				<soap12:body use="literal" />
			</wsdl:output>
		</wsdl:operation>
		<wsdl:operation name="PostPublication">
			<soap12:operation soapAction="http://www.openoandm.org/xml/ISBM/PostPublication" style="document" />
			<wsdl:input>
				<soap12:body use="literal" />
			</wsdl:input>
			<wsdl:output>
				<soap12:body use="literal" />
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>
	<wsdl:service name="ISBMProviderPublicationService">
		<wsdl:port name="ISBMProviderPublicationServiceSoap" binding="tns:ISBMProviderPublicationServiceSoap">
			<soap:address location="http://localhost:9080/ISBMProviderPublicationService.asmx" />
		</wsdl:port>
		<wsdl:port name="ISBMProviderPublicationServiceSoap12" binding="tns:ISBMProviderPublicationServiceSoap12">
			<soap12:address location="http://localhost:9080/ISBMProviderPublicationService.asmx" />
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>